local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local player = game.Players.LocalPlayer
local base = workspace.Maps:WaitForChild(player.Name .. "Tycoon")
local PlacedBoxes = base:WaitForChild("PlacedBoxes")
local Fling = workspace.FlingModels
local DeleteMob = false
local AutoFarm = false

local Window = Rayfield:CreateWindow({
	Name = "NikeHub",
	Icon = 0,
	LoadingTitle = "Script Simples Com AutoFarm",
	LoadingSubtitle = "by ericknick",
	ShowText = "Nike Hub",
	Theme = "Default",
	ToggleUIKeybind = "K",

	ConfigurationSaving = {
		Enabled = false,
		FileName = "Big Hub"
	}
})

local Tab = Window:CreateTab("Main", 4483362458)

local Button = Tab:CreateButton({
	Name = "Unlock All Shop (No Rebirth)",
	Callback = function()
		local Player = game.Players.LocalPlayer
		local Leaderstats = Player:WaitForChild("leaderstats", 3)
		if Leaderstats then
			local Rebirth = Leaderstats:WaitForChild("Rebirth", 5)
			if Rebirth then
				Rebirth.Value += 100
				task.wait(20)
				Rebirth.Value -= 100
			end
		end
	end,
})

local Toggle = Tab:CreateToggle({
    Name = "AutoFarm (Hammer)",
    CurrentValue = false,
    Flag = "AutoFarmToggle",
    Callback = function(Value)
        AutoFarm = Value
    end,
})

local Toggle1 = Tab:CreateToggle({
    Name = "Ant lag (Delete Mobs)",
    CurrentValue = false,
    Flag = "DeleteMob",
    Callback = function(Value)
        DeleteMob = Value
    end,
})

while true do
    task.wait(0.1)
	if DeleteMob then
		for _, obj in ipairs(Fling:GetChildren()) do
			obj:Destroy()
		end
	end
    if AutoFarm then

        local function processarModelos(func)
            local checked = {}
            local nextModel = PlacedBoxes:FindFirstChildWhichIsA("Model")
            while nextModel do
                if not checked[nextModel] then
                    checked[nextModel] = true
                    func(nextModel)
                end

                nextModel = nil
                for _, child in ipairs(PlacedBoxes:GetChildren()) do
                    if child:IsA("Model") and not checked[child] then
                        nextModel = child
                        break
                    end
                end
            end
        end

        processarModelos(function(model)
            if not model.PrimaryPart then
                local part = model:FindFirstChildWhichIsA("BasePart")
                if part then
                    model.PrimaryPart = part
                end
            end
        end)

        local char = player.Character or player.CharacterAdded:Wait()
        local hrp = char:WaitForChild("HumanoidRootPart")

        processarModelos(function(model)
            if model.PrimaryPart then
                model:SetPrimaryPartCFrame(hrp.CFrame * CFrame.new(0, -2, -2))
            end
        end)
    end
	local player = game.Players.LocalPlayer
	local AutoClikerLegit = player:WaitForChild("PlayerData", 3)

	if AutoClikerLegit and AutoFarm == true then
		AutoClikerLegit:FindFirstChild("AutoSlash").Value = true
	else
		AutoClikerLegit:FindFirstChild("AutoSlash").Value = false
	end
	local args = { player }
	game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("SlashEvent"):FireServer(unpack(args))
end
