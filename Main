local UIS = game:GetService("UserInputService")
local player = game.Players.LocalPlayer
local PlayerGui = player:WaitForChild("PlayerGui")

-- üîç Procurar Tycoon do jogador usando a propriedade TycoonOwner
local Maps = workspace:WaitForChild("Maps")
local base = nil

for _, tycoon in ipairs(Maps:GetChildren()) do
	if tycoon:IsA("Model") and tycoon:FindFirstChild("TycoonOwner") then
		if tycoon.TycoonOwner.Value == player then
			base = tycoon
			break
		end
	end
end

if not base then
	warn("Tycoon do jogador n√£o encontrado!")
	return
end

local PlacedBox = base:WaitForChild("PlacedBoxes")

-- üîò Criar GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "TeleportGUI"
screenGui.Parent = PlayerGui

local mobileBtn = Instance.new("TextButton")
mobileBtn.Size = UDim2.new(0, 180, 0, 70)
mobileBtn.Position = UDim2.new(0.5, -90, 0.8, 0)
mobileBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
mobileBtn.Text = "Teleportar"
mobileBtn.TextScaled = true
mobileBtn.Parent = screenGui

local corner = Instance.new("UICorner")
corner.CornerRadius = UDim.new(0, 12)
corner.Parent = mobileBtn

